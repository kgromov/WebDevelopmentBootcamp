<%- include("partials/header") -%>

<h1>Compose</h1>
<form class="form-signin" action="/compose" method="post">
    <div class="form-group">
        <label for="postTitle">Title</label>
        <input class="form-control" type="text" name="postTitle">
        <label for="postBody">Post</label>
        <textarea class="form-control" name="postBody" id="" cols="30" rows="4"></textarea>
    </div>      
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>  

<%- include("partials/footer") -%>